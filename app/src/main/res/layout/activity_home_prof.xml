<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home_prof">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="443dp"
        android:layout_height="747dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/back_white" />

    <TextView
        android:id="@+id/text_state"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="상태"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="270dp"
        tools:layout_editor_absoluteY="422dp" />

    <EditText
        android:id="@+id/username_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:background="@null"
        android:ems="10"
        android:hint="유저이름"
        android:inputType="textPersonName"
        android:textSize="23sp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="62dp" />

    <TextView
        android:textColor="#568A35"
        android:id="@+id/textview2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="의 식물"
        android:textSize="23sp"
        tools:layout_editor_absoluteX="224dp"
        tools:layout_editor_absoluteY="62dp" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="107dp"
        android:layout_height="101dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@drawable/flower"
        tools:layout_editor_absoluteX="67dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="107dp"
        android:layout_height="101dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/flower" />

    <TextView
        android:textColor="#568A35"
        android:id="@+id/mypage_bnt3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="16dp"
        android:text="마이페이지"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="107dp"
        android:layout_height="101dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.611"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/flower" />

    <TextView
        android:textColor="#568A35"
        android:id="@+id/shop_bnt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="쇼핑하기"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="699dp" />

    <TextView
        android:textColor="#568A35"
        android:id="@+id/diary_bnt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="일기쓰기"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="206dp" />

    <TextView
        android:id="@+id/text_water"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="수분"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="422dp" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="106dp"
        android:layout_height="98dp"
        app:srcCompat="@drawable/water"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="324dp" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="90dp"
        android:layout_height="90dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.505"
        app:srcCompat="@drawable/state"
        tools:layout_editor_absoluteX="247dp" />

    <TextView
        android:id="@+id/water_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="370dp" />

    <TextView
        android:id="@+id/state_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="259dp"
        tools:layout_editor_absoluteY="364dp" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="133dp"
        android:layout_height="135dp"
        app:srcCompat="?android:attr/alertDialogIcon"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="147dp" />

</androidx.constraintlayout.widget.ConstraintLayout>