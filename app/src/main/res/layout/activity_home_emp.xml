<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home_emp">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="443dp"
        android:layout_height="747dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/back_white" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="210dp"
        android:layout_height="94dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/save_white"
        tools:layout_editor_absoluteY="188dp" />

    <EditText
        android:id="@+id/username_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:ems="10"
        android:hint="유저이름"
        android:inputType="textPersonName"
        android:textSize="23sp"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="62dp" />

    <TextView
        android:textColor="#568A35"
        android:id="@+id/textview"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="님 반갑습니다."
        android:textSize="23sp"
        tools:layout_editor_absoluteX="193dp"
        tools:layout_editor_absoluteY="62dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="식물이 등록되지 않았어요."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="132dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="107dp"
        android:layout_height="101dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/flower" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="107dp"
        android:layout_height="101dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView9"
        app:layout_constraintHorizontal_bias="0.642"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:srcCompat="@drawable/flower" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="107dp"
        android:layout_height="101dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@drawable/flower"
        tools:layout_editor_absoluteX="59dp" />

    <TextView
        android:textColor="#568A35"
        android:id="@+id/shop_bnt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="쇼핑하기"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="699dp" />

    <TextView
        android:textColor="#568A35"
        android:id="@+id/diary_bnt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="일기쓰기"
        tools:layout_editor_absoluteX="197dp"
        tools:layout_editor_absoluteY="699dp" />

    <TextView
        android:textColor="#568A35"
        android:id="@+id/mypage_bnt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="마이페이지"
        tools:layout_editor_absoluteX="309dp"
        tools:layout_editor_absoluteY="699dp" />

</androidx.constraintlayout.widget.ConstraintLayout>